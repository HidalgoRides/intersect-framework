{
  "name": "hidalgo-rides/intersect-framework",
  "description": "Web framework built for making dev life a little easier.",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "intersect",
    "framework",
    "hidalgo"
  ],
  "authors": [
    {
      "name": "Tim Kipp",
      "email": "timkipp.22.developer@gmail.com"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "App\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Tests\\" : "tests/"
    }
  },
  "require": {
    "hidalgo-rides/intersect-web" : "^1.0.0",
    "php": "^7.1.3",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
